<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="226.0" prefWidth="416.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lab6.controller.LoginController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <AnchorPane prefHeight="154.0" prefWidth="438.0">
        <children>
            <Label contentDisplay="CENTER" layoutX="21.0" layoutY="19.0" prefHeight="26.0" prefWidth="39.0" text="Id" textAlignment="CENTER">
            <font>
               <Font name="Ebrima Bold" size="16.0" />
            </font></Label>
            <TextField fx:id="loginField" layoutX="70.0" layoutY="15.0" maxHeight="-Infinity" maxWidth="-Infinity" onInputMethodTextChanged="#loginLabel" prefHeight="34.0" prefWidth="121.0" />
            <Button layoutX="99.0" layoutY="77.0" mnemonicParsing="false" onAction="#handleLogin" prefHeight="34.0" prefWidth="62.0" text="Login">
            <font>
               <Font name="Ebrima Bold" size="12.0" />
            </font></Button>
            <Button layoutX="253.0" layoutY="15.0" mnemonicParsing="false" onAction="#handleCreateAccountDialog" prefHeight="34.0" prefWidth="114.0" text="Create account">
            <font>
               <Font name="Ebrima Bold" size="13.0" />
            </font></Button>
            <Button layoutX="253.0" layoutY="77.0" mnemonicParsing="false" onAction="#onHandleUsers" prefHeight="34.0" prefWidth="114.0" text="All users">
            <font>
               <Font name="Ebrima Bold" size="12.0" />
            </font></Button>
        </children>
    </AnchorPane>
</VBox>
