<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="183.0" prefWidth="471.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lab6.controller.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <AnchorPane prefHeight="212.0" prefWidth="543.0">
        <children>
            <Label layoutX="49.0" layoutY="28.0" text="Id" />
            <TextField fx:id="loginField" layoutX="91.0" layoutY="23.0" maxHeight="-Infinity" maxWidth="-Infinity" onInputMethodTextChanged="#loginLabel" prefHeight="26.0" prefWidth="100.0" />
            <Button layoutX="118.0" layoutY="64.0" mnemonicParsing="false" onAction="#handleLogin" text="Login" />
            <Button layoutX="253.0" layoutY="24.0" mnemonicParsing="false" onAction="#handleCreateAccountDialog" text="Create account" />
            <Button layoutX="271.0" layoutY="64.0" mnemonicParsing="false" text="All users" />
        </children>
    </AnchorPane>

    <Label fx:id="welcomeText" />
    <Button onAction="#onHelloButtonClick" text="Hello!" />
</VBox>
