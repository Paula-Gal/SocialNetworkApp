<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="396.0" prefWidth="526.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lab6.controller.AddFriendController">
   <children>
      <TextField fx:id="searchField" layoutX="64.0" layoutY="49.0" onAction="#onSearchFieldAction" />
      <Button fx:id="searchButton" layoutX="246.0" layoutY="48.0" mnemonicParsing="false" onAction="#onHandleSearch" prefHeight="26.0" prefWidth="63.0" text="Search">
         <font>
            <Font name="Ebrima" size="13.0" />
         </font></Button>
      <TableView fx:id="tableViewUsers" layoutX="64.0" layoutY="115.0" prefHeight="200.0" prefWidth="363.0">
        <columns>
          <TableColumn fx:id="tableColumnFirstName" prefWidth="115.0" text="First Name" />
          <TableColumn fx:id="tableColumnLastName" minWidth="0.0" prefWidth="118.0" text="Last Name" />
        </columns>
      </TableView>
      <Button layoutX="44.0" layoutY="341.0" mnemonicParsing="false" onAction="#onHandleBack" prefHeight="26.0" prefWidth="63.0" text="Back">
         <font>
            <Font name="Ebrima" size="13.0" />
         </font></Button>
   </children>
</AnchorPane>
