<AnchorPane prefHeight="598.0" prefWidth="975.0" stylesheets="/style/messages.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lab6.controller.MessagesController">
   <children>
      <VBox layoutY="3.0" prefHeight="592.0" prefWidth="368.0" stylesheets="/style/messages.css">
         <children>
            <AnchorPane fx:id="anchorPagination" prefHeight="513.0" prefWidth="382.0" stylesheets="/style/messages.css">
               <children>
                  <Button fx:id="createGroupButton" layoutX="233.0" layoutY="443.0" mnemonicParsing="false" onAction="#onCreateGroup" prefHeight="30.0" prefWidth="122.0" styleClass="button-messages" text="Create group" textAlignment="RIGHT" />
                  <Line endX="210.0" layoutX="172.0" layoutY="491.0" startX="-172.0" styleClass="vl" />
                  <ImageView fx:id="addImage" fitHeight="19.0" fitWidth="15.0" layoutX="239.0" layoutY="449.0" pickOnBounds="true" preserveRatio="true" styleClass="image-add" />
               </children>
            </AnchorPane>
            <HBox prefHeight="93.0" prefWidth="368.0" spacing="40.0" stylesheets="/style/messages.css">
               <children>
                  <Button mnemonicParsing="false" onAction="#openPrivateMessages" prefHeight="47.0" prefWidth="145.0" styleClass="button-messages" text="Private Messages">
                     <HBox.margin>
                        <Insets left="25.0" top="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#openGroups" prefHeight="47.0" prefWidth="145.0" styleClass="button-messages" text="Groups">
                     <HBox.margin>
                        <Insets right="10.0" top="10.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
      <VBox layoutX="385.0" layoutY="3.0" prefHeight="592.0" prefWidth="588.0">
         <children>
            <HBox prefHeight="70.0" prefWidth="588.0" spacing="40.0" stylesheets="/style/messages.css">
               <children>
                  <ImageView fx:id="userImage" fitHeight="63.0" fitWidth="90.0" pickOnBounds="true" preserveRatio="true" />
                  <Label fx:id="userName" prefHeight="57.0" prefWidth="352.0" styleClass="input100" text="Label" />
               </children>
            </HBox>
            <AnchorPane prefHeight="440.0" prefWidth="588.0" stylesheets="/style/messages.css" />
            <HBox prefHeight="100.0" prefWidth="200.0" spacing="20.0" stylesheets="/style/messages.css">
               <children>
                  <TextField fx:id="conversationField" prefHeight="59.0" prefWidth="421.0" styleClass="conversation-field">
                     <HBox.margin>
                        <Insets bottom="25.0" left="50.0" top="20.0" />
                     </HBox.margin>
                  </TextField>
                  <ImageView fx:id="imageSend" fitHeight="47.0" fitWidth="56.0" pickOnBounds="true" preserveRatio="true" styleClass="image-send">
                     <HBox.margin>
                        <Insets bottom="25.0" left="20.0" right="25.0" top="20.0" />
                     </HBox.margin>
                  </ImageView>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
      <Line endX="-100.0" endY="582.0" layoutX="485.0" layoutY="13.0" startX="-100.0" startY="-10.0" styleClass="vl" />
      <VBox alignment="BOTTOM_LEFT" layoutX="1.0" layoutY="-2.0" prefHeight="94.0" prefWidth="382.0">
         <children>
            <Label prefHeight="36.0" prefWidth="205.0" styleClass="input100" text="Messages">
