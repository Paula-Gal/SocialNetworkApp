<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="433.0" prefWidth="746.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lab6.controller.UserController">
    <children>
        <Label fx:id="userLabel" layoutX="32.0" layoutY="83.0" prefHeight="32.0" prefWidth="259.0" />
        <TableView fx:id="tableViewFriends" layoutX="322.0" layoutY="117.0" prefHeight="207.0" prefWidth="383.0">
            <columns>
                <TableColumn fx:id="tableColumnFriendFirstName" prefWidth="98.0" text="First Name" />
            <TableColumn fx:id="tableColumnFriendLastName" prefWidth="75.0" text="Last Name" />
                <TableColumn fx:id="tableColumnFriendDate" prefWidth="209.0" text="Friendship Date" />
            </columns>
        </TableView>
        <Button layoutX="42.0" layoutY="161.0" mnemonicParsing="false" onAction="#handleAddFriend" text="Add friend" />
        <Button layoutX="197.0" layoutY="161.0" mnemonicParsing="false" onAction="#handleRemoveFriend" text="Remove friend" />
      <Button layoutX="42.0" layoutY="221.0" mnemonicParsing="false" onAction="#handleShowFriends" text="Show the friends" />
      <Button layoutX="42.0" layoutY="271.0" mnemonicParsing="false" onAction="#handleShowRequests" text="Show requests" />
      <Button layoutX="42.0" layoutY="324.0" mnemonicParsing="false" onAction="#handleMessages" text="Messages" />
    </children>
</AnchorPane>
